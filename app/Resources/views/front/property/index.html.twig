                                    <div class="overlay">
                                        <a href="#" class="btn btn-detail">Detail</a>
                                    </div>
                                </figure>
                                <div class="item-data">
                                    <div class="item-header clearfix">
                                        <h3><a href="#">Los Angles Apartment</a></h3>
